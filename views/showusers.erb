<table class="table">
  <thead>
    <tr>
      <th scope="col"></th>
      <th scope="col">Name</th>
      <th scope="col">Phone</th>
      <th scope="col">Date</th>
      <th scope="col">Doctor</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |row|%>
      <tr>
        <th scope="row"></th>
        <td><%= row['username'] %></td>
        <td><%=row['phone'] %></td>
        <td><%=row['datestamp'] %></td>
        <td><%=row['doctor'] %></td>
      </tr>
    <% end %>
  </tbody>
</table>